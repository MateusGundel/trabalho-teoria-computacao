                                  </content>
                              </Tab>
                              <Tab closable="false" text="Passo 4">
                                  <content>
                                      <VBox alignment="TOP_CENTER" spacing="10.0">
                                          <children>
                                              <HBox spacing="10.0" VBox.vgrow="SOMETIMES">
                                                  <VBox.margin>
                                                      <Insets />

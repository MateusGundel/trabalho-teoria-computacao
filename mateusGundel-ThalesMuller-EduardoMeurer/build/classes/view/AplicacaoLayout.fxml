                      <Button fx:id="btnVerificarEqui" alignment="TOP_LEFT" mnemonicParsing="false" text="Verificar Equivalência">
                          <opaqueInsets>
                              <Insets />
                          </opaqueInsets>
                      </Button>
                      <Button fx:id="btnCancelar" mnemonicParsing="false" text="Cancelar" />
                      <Button fx:id="btnSair" mnemonicParsing="false" text="Sair" />
                  </buttons>
                  <padding>
                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </padding>
              </ButtonBar>
         </children>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </VBox>

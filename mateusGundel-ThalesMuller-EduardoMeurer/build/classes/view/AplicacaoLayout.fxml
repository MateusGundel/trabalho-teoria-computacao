    <center>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Entradas" />
            <HBox>
               <children>
                      <VBox alignment="CENTER">
                          <children>
                              <Label text="Programa 1" />
                              <TextArea fx:id="entradaProgramaUm" VBox.vgrow="ALWAYS">
                                  <VBox.margin>
                                      <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
                                  </VBox.margin>
                              </TextArea>
                          </children>
                      </VBox>
                      <VBox alignment="CENTER">
                          <children>
                              <Label text="Programa 2" />
                              <TextArea fx:id="entradaProgramaDois" VBox.vgrow="ALWAYS">
                                  <VBox.margin>
                                      <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
                                  </VBox.margin>
                              </TextArea>
                          </children>
                      </VBox>
               </children>
            </HBox>

<BorderPane prefHeight="600.0" prefWidth="800.0" stylesheets="@aplicacao.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AppController">
    <bottom>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
              <VBox alignment="CENTER" prefHeight="271.0" prefWidth="800.0">
                  <children>
                      <Label text="Resultados" />
                      <TabPane tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
                          <tabs>
                              <Tab closable="false" text="Passo 1">
                                  <content>
                                      <VBox alignment="TOP_CENTER" spacing="10.0">
                                          <children>
                                              <HBox spacing="10.0" VBox.vgrow="SOMETIMES">
                                                  <VBox.margin>
                                                      <Insets />
                                                  </VBox.margin>
                                                  <children>
                                                      <VBox prefWidth="150.0" HBox.hgrow="ALWAYS">
                                                          <children>
                                                              <Label text="Programa 1" />
                                                              <TextArea fx:id="resultadoPasso1Programa1" editable="false" VBox.vgrow="SOMETIMES" />
                                                          </children>
                                                      </VBox>
                                                      <VBox prefWidth="150.0" HBox.hgrow="ALWAYS">
                                                          <children>
                                                              <Label text="Programa 2" />
                                                              <TextArea fx:id="resultadoPasso1Programa2" editable="false" VBox.vgrow="SOMETIMES" />
                                                          </children>
                                                      </VBox>
                                                  </children>
                                              </HBox>
                                          </children>
                                          <padding>
                                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                          </padding>
                                      </VBox>
                                  </content>
                              </Tab>
                              <Tab closable="false" text="Passo 2">
                                  <content>
                                      <VBox alignment="TOP_CENTER" spacing="10.0">
                                          <children>
                                              <HBox spacing="10.0" VBox.vgrow="SOMETIMES">
